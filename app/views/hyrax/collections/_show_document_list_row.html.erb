<% id = document.id %>
<tr id="document_<%= id %>">
  <td>
    <div class="media">
      <div class="media-body">
        <p>
          <%= link_to document.title_or_label,
                              [main_app, document],
                              id: "src_copy_link#{id}",class: "underline-link" %>
        </p>
      </div>
    </div>
  </td>
  <td><%= document.agency %></td>
  <td><%= document.date_published %></td>
</tr>
